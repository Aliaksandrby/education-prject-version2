    <changeSet id="1" author="Aliaksandrby">
        <sql>
            CREATE TABLE IF NOT EXISTS t_car (
                id INT NOT NULL PRIMARY KEY,
                name VARCHAR(200),
                year INT,
                engine_description VARCHAR(200),
                transmission VARCHAR(100),
                price INT
            );

            CREATE TABLE IF NOT EXISTS t_car_seq (
            next_val INT UNSIGNED PRIMARY KEY NOT NULL
            );
            INSERT INTO t_car_seq VALUES (0);

            CREATE TABLE IF NOT EXISTS t_image_car (
                id INT NOT NULL PRIMARY KEY,
                car_id INT,
                path_image VARCHAR(100),
                FOREIGN KEY (car_id) REFERENCES t_car (id)
            );

            CREATE TABLE IF NOT EXISTS t_image_car_seq (
            next_val INT UNSIGNED PRIMARY KEY NOT NULL
            );
            INSERT INTO t_image_car_seq VALUES (0);
        </sql>
    </changeSet>
